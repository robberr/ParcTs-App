{
  "name": "parc-ts-app",
  "version": "1.0.0",
  "description": "ParcTs-App",
  "main": "server.js",
  "scripts": {
    "tes": "echo \"Error: no test specified\" && exit 1",
    "star": "parcel ./index.html --open",
    "stt": "parcel src/index.pug --open",
    "buil": "parcel build src/index.pug -d docs --public-url ./",
    "com": "tsc app.ts",
    "tsc": "tsc",
    "start": "node dist/server.js",
    "de1": "nodemon src/server.ts",
    "build": "tsc -p .",
    "dev": "nodemon src",
    "det": "nodemon --exec \"ts-node\" --cache-directory .tscache ./src/server.ts"
  },
  "author": {
    "name": ""
  },
  "devDependencies": {
    "@types/node": "^8.0.14",
    "parcel-bundler": "^1.12.4",
    "pug": "^3.0.0",
    "sass": "^1.27.0",
    "typescript": "^4.0.3"
  }
}
